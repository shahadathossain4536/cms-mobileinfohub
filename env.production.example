# Production Environment Variables
# Copy this file to .env.production and configure

# API Base URL - Change this to your production API URL
REACT_APP_API_BASE_URL=https://deviceinfohub-server.vercel.app/api

# Or if your API is on the same VPS with subdomain:
# REACT_APP_API_BASE_URL=https://api.yourdomain.com/api

# Port (only used in development)
PORT=3322

# Node environment
NODE_ENV=production

